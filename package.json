  },
  "keywords": [
    "node.js",
    "webpack",
    "expressjs",
    "javascript",
    "cicd"
  ],
  "author": "1ProCrafter",
  "license": "MIT",
  "devDependencies": {
    "ejs": "^2.6.2",
    "express": "^4.18.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "path": "^0.12.7"
  }
}