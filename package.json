{
  "name": "express-webpack-example",
  "version": "0.0.1",
  "description": "Repo showing how to bundle an express.js application to a bundle with webpack",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "dist-start": "node dist/app.js",
    "build": "webpack --config build/webpack.prod.conf.js",
